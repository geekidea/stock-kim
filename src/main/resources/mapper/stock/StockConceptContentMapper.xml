<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.geekidea.stock.mapper.StockConceptContentMapper">

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, concept_batch_no, stock_code, stock_name, concept_content, concept_count, create_time, update_time
    </sql>
    
    <select id="getStockConceptContentById" resultType="io.geekidea.stock.dto.vo.StockConceptContentQueryVo">
        select
        <include refid="Base_Column_List"/>
        from stock_concept_content where id = #{id}
    </select>
        
    <select id="getStockConceptContentPageList" resultType="io.geekidea.stock.dto.vo.StockConceptContentQueryVo">
        select
        <include refid="Base_Column_List"/>
        from stock_concept_content
    </select>
    
</mapper>
